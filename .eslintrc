{
  "env": {
    "browser": true,
    "node": true,
    "jest": true,
    "es6": true
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    },
  "plugins": ["react"],
  "rules": {
    "quotes": ["error", "single"],
    "semi": ["error", "always"],
    "react/jsx-uses-vars": [2],
    "react/prop-types": 0,
    "no-console": 1
  }
}